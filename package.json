{
  "name": "vitetail",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@7.8.0",
  "description": "Vite Starter template with TailwindCSS + DaisyUI",
  "author": "compilekaiten",
  "license": "MIT",
  "scripts": {
    "build": "vite-ssg build",
    "tbuild": "pnpm lint && pnpm typecheck && vite-ssg build",
    "dev": "vite --port 3333",
    "open": "vite --port 3333 --open",
    "lint": "eslint .",
    "preview": "vite preview",
    "preview-https": "serve dist",
    "test": "vitest",
    "test:e2e": "cypress open",
    "test:unit": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "dep": "taze major -IR"
  },
  "dependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/typography": "^0.5.7",
    "@types/node": "18.8.0",
    "@vueuse/core": "^9.3.1",
    "@vueuse/head": "^0.9.8",
    "daisyui": "^2.31.0",
    "pinia": "^2.0.23",
    "tailwind-scrollbar": "2.1.0-preview.0",
    "theme-change": "^2.2.0",
    "unplugin-icons": "^0.14.12",
    "vue": "^3.2.41",
    "vue-demi": "^0.13.11",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.27.0",
    "@iconify/json": "^2.1.123",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/test-utils": "^2.1.0",
    "autoprefixer": "^10.4.12",
    "critters": "^0.0.16",
    "cross-env": "^7.0.3",
    "cypress": "^10.10.0",
    "eslint": "^8.25.0",
    "eslint-plugin-cypress": "^2.12.1",
    "https-localhost": "^4.7.1",
    "pnpm": "^7.13.5",
    "postcss": "^8.4.18",
    "tailwindcss": "^3.1.8",
    "taze": "^0.8.3",
    "typescript": "^4.8.4",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.8",
    "vite": "^3.1.8",
    "vite-plugin-inspect": "^0.7.5",
    "vite-plugin-pages": "^0.27.0",
    "vite-plugin-pwa": "^0.13.1",
    "vite-plugin-vue-component-preview": "^0.3.3",
    "vite-plugin-vue-layouts": "^0.7.0",
    "vite-ssg": "^0.21.1",
    "vite-ssg-sitemap": "^0.4.3",
    "vitest": "^0.24.3",
    "vue-tsc": "^1.0.8"
  }
}
